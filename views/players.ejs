<% include partials/header %>

<h1>
        <%= title %>
</h1>

<table>
        <thead>
          <tr>
            <th>Name</th>
            <th>Team</th>
            <th>Jersey#</th>
          </tr>
        </thead>
        <tbody>
            <% for (let i = 0; i < players.length; i++) { %>
          <tr>
            <td><%= players[i].firstName.substring(0,1) %>. <%= players[i].lastName %></td>
            <td><a href="<%= players[i].url %>"><%= players[i].team %></a></td>
            <td><%= players[i].jersey %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
<% include partials/footer %>